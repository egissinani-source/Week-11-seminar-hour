num1=float(input("Enter your first number :"))
num2=float(input("Enter your second number :"))
num3=float(input("Enter your third number :"))
largest = num1
if num2> largest :
    largest = num2
if num3 > largest :
    largest = num3
print("Between" ,num1 , num2, num3, "," , largest, "is the largest number")
